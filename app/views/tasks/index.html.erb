<h1>My tasks</h1>

<div class="tasks">
  <ul>
    <% @tasks.each do |task| %>
      <% if task.completed == true %>
      <li><i class="far fa-check-square"></i> <%= task.title%></li>
      <% else %>
      <li><i class="far fa-square"></i> <%= task.title%></li>
      <% end%>
    <% end %>
  </ul>
</div




